		<script type="text/javascript">
			var scoreArr = scoreStr.split(';').join(':').split(':');

var sumInt = 0;

var j = 0;

for(var i=0;i<=scoreArr.length;i++){

  if(!isNaN(scoreArr[i])){

    sumInt += parseInt(scoreArr[i]);

    j++;

  }

}

document.write(Math.round(sumInt/j));
		</script>
